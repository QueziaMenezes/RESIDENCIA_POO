package br.com.poo.contas;

public class Conta {

	// o que uma conta tem
	public int numero;
	public String titular;
	public double saldo;

	// ... outros atributos da classe ...
	public void sacar
	double valor)
	{
		double novoSaldo = this.saldo - valor;
		this.saldo = novoSaldo;
	}

	// ... outros m√©todos e atributos da classe ...
	public boolean sacar(double valor) {
		if (this.saldo < valor) {
			return false;
		} else {
			double novoSaldo = this.saldo - valor;
			this.saldo = novoSaldo;

			return true;
		}

	}

}
